{
    "configurations": [
        {
            "name": "Win32",
            "includePath": [
                "${workspaceRoot}/**",
                "${env:programfiles(x86)}/Arduino/hardware/arduino/avr/cores/**",
                "${env:programfiles(x86)}/Arduino/hardware/arduino/avr/libraries/**",
                "${env:programfiles(x86)}/Arduino/hardware/arduino/variants/standard",
                // "${env:programfiles(x86)}/Arduino/tools/**",
                // "${env:programfiles(x86)}/Arduino/hardware/tools/**",
                // "${env:programfiles(x86)}/Arduino/libraries/**",
                // "${env:userprofile}/Documents/Arduino/hardware/tools/**",
                "${env:userprofile}/Documents/Arduino/libraries/**"
            ],
            "forcedInclude": [
                "${env:programfiles(x86)}/Arduino/hardware/arduino/avr/cores/arduino/Arduino.h"
            ],
            "intelliSenseMode": "gcc-x86",
            "compilerPath": "${env:programfiles(x86)}/Arduino/hardware/tools/avr/bin/avr-gcc.exe",
            "cStandard": "c11",
            "cppStandard": "c++17",
            "defines": [
                "USBCON"
            ]
        }
    ],
    "version": 4
}